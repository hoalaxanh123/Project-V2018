        <div class="support-hotline text-danger">
            Hỗ trợ web<br><span class="ng-binding ">0167.552.8766</span>
        </div>
        <ul class="text-center">
            <!-- ngIf: item.Skype!=''&&item.Skype!=null -->
            @{
